{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/web/dist/$1"
    }
  ],
  "buildCommand": "cd apps/web && pnpm install && pnpm run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "apps/web/dist/**/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}
